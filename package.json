{
  "name": "fe-life",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.7.0",
  "description": "The frontend life record of ntnyq",
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "build": "vitepress build docs",
    "dev": "vitepress dev docs",
    "lint": "run-p \"lint:*\"",
    "lint:cspell": "cspell \"**\"",
    "lint:script": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs .",
    "prepare": "husky install",
    "pretty": "prettier --write \"**/*.{md,js,ts,css,scss,vue}\"",
    "preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "2.0.0-beta.16",
    "@ntnyq/prettier-config": "1.12.4",
    "@qxy/cspell-dict": "^0.0.3",
    "@vite-pwa/vitepress": "^0.2.0",
    "cspell": "^7.1.1",
    "eslint": "^8.48.0",
    "eslint-define-config": "^1.23.0",
    "husky": "^8.0.3",
    "markdown-it": "^13.0.1",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.7.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vitepress": "1.0.0-rc.10",
    "workbox-window": "^7.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,vue,json,md,yml,yaml}": "pnpm run lint:script --fix"
  }
}
