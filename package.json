{
  "name": "fe-life",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "description": "The frontend life record of ntnyq",
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "build": "vitepress build docs",
    "dev": "vitepress dev docs",
    "lint": "run-p lint:*",
    "lint:cspell": "cspell \"**\"",
    "lint:script": "eslint --cache .",
    "prepare": "husky install",
    "pretty": "prettier --write \"**/*.{md,js,ts,css,scss,vue}\"",
    "preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.7.3",
    "@ntnyq/prettier-config": "^1.7.3",
    "@qxy/cspell-dict": "^0.0.3",
    "@vite-pwa/vitepress": "^0.0.5",
    "cspell": "^6.31.1",
    "eslint": "^8.40.0",
    "flexsearch": "^0.7.31",
    "husky": "^8.0.3",
    "markdown-it": "^13.0.1",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.5.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-pwa": "^0.14.7",
    "vitepress": "1.0.0-alpha.75",
    "workbox-window": "^6.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,vue,json,md,yml,yaml}": "eslint --fix"
  }
}
