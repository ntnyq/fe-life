    "lint": "run-p \"lint:*\"",
    "lint:cspell": "cspell \"**\"",
    "lint:script": "eslint .",
    "prepare": "husky",
    "pretty": "prettier --write \"**/*.{md,js,ts,css,scss,vue}\"",
    "preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.20.2",
    "@ntnyq/prettier-config": "1.16.0",
    "@qxy/cspell-dict": "^0.0.3",
    "@vite-pwa/vitepress": "^0.3.1",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "husky": "^9.0.6",
    "markdown-it": "^14.0.0",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.14.3",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.4",
    "vitepress": "1.0.0-rc.40",
    "workbox-window": "^7.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,vue,json,md,yml,yaml}": "eslint --fix"
  }
