{
  "name": "fe-life",
  "version": "1.0.0",
  "private": true,
  "description": "The frontend life record of ntnyq",
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "build": "vitepress build docs",
    "dev": "vitepress dev docs --open --host",
    "lint": "run-p lint:*",
    "lint:cspell": "cspell \"**\"",
    "pretty": "prettier --write \"**/*.{md,js,ts,css,scss,vue}\""
  },
  "lint-staged": {
    "*.{js,vue,json,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "prettier": "@ntnyq/prettier-config",
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^1.0.7",
    "@qxy/cspell-dict": "^0.0.3",
    "cspell": "^6.14.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.14.2",
    "prettier": "^2.7.1",
    "sort-package-json": "^2.0.0",
    "vitepress": "1.0.0-alpha.27"
  },
  "packageManager": "pnpm@7.14.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  }
}
