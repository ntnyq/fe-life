{
  "name": "fe-life",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.28.0",
  "description": "The frontend life record of ntnyq",
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "build": "vitepress build docs",
    "dev": "vitepress dev docs",
    "lint": "run-p lint:*",
    "lint:cspell": "cspell \"**\"",
    "lint:script": "eslint --cache .",
    "prepare": "husky install",
    "pretty": "prettier --write \"**/*.{md,js,ts,css,scss,vue}\""
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.4.0",
    "@ntnyq/prettier-config": "^1.4.0",
    "@qxy/cspell-dict": "^0.0.3",
    "cspell": "^6.28.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.28.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vitepress": "1.0.0-alpha.49"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,vue,json,md,yml,yaml}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
