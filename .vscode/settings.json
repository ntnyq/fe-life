{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "prettier.configPath": "./prettier.config.mjs",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "eslint.validate": [
    "vue",
    "yaml",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.useGitignore": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "achrinza",
    "aliyun",
    "alloc",
    "antd",
    "antfu",
    "antv",
    "cdnurl",
    "CRBNZ",
    "czvf",
    "deffered",
    "dgst",
    "dpkg",
    "fluro",
    "formz",
    "fuser",
    "FZCYSK",
    "gstatic",
    "hunantv",
    "iconify",
    "iphoneos",
    "jsesc",
    "jsweixin",
    "krpano",
    "livetv",
    "mkdir",
    "mlly",
    "mmin",
    "mysqld",
    "nolebase",
    "noopener",
    "noreferrer",
    "novtt",
    "npmrc",
    "occured",
    "onbeforeunload",
    "paren",
    "pbcopy",
    "pinia",
    "rtmp",
    "tsup",
    "ueditor",
    "unjs",
    "unocss",
    "vant",
    "videojs",
    "vitepress",
    "vueuse",
    "weapp",
    "xcassets",
    "xzvf",
    "zhihu"
  ]
}
